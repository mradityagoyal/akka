  <component name="FindInProjectRecents">
    <findStrings>
      <find>package com\.packt\.chapter1\n\nimport akka\.actor\.Actor\n\nimport scala\.collection\.mutable\nimport scala\.math\._\n\n\/\*\*\n  \* Created by addy on 7\/14\/17\.\n  \*\/\nclass PowerSumActor extends Actor \{\n\n  override def receive\: Receive \= \{\n    case \(x\: Int\, y\: Int\) \=\&gt; \{\n      println\(s\&quot;Number of ways you can write \$x as a sum of unique digits raise to power of \$y is \$\{numberOfWays\(x\, y\)\}\&quot;\)\n      \/\/      print\(dict\)\n    \}\n  \}\n\n\n  val dict \= new mutable\.HashMap\[Int\, PowerSumResult\]\n\n\n  case class PowerSumResult\(number\: Int\, maxInSeries\: Int\, result\: Int\) extends Ordered\[PowerSumResult\] \{\n    import scala\.math\.Ordered\.orderingToOrdered\n    def compare\(that\: PowerSumResult\)\: Int \= this\.maxInSeries compare that\.maxInSeries\n  \}\n\n\n  def numberOfWays\(X\: Int\, N\: Int\)\: Int \= \{\n    def numWays\(sum\: Int\, maximum\: Int\)\: Int \= sum match \{\n      case x if x \&lt; 1 \=\&gt; 0\n      case _ \=\&gt; \{\n        val flooredRoot \= floor\(pow\(sum\, 1\.0 \/ N\)\)\.toInt\n        if \(maximum \&gt; flooredRoot\) \{\n          numWays\(sum\, flooredRoot\)\n        \} else \{\n          val limit \= min\(maximum\, flooredRoot\)\n          val precalc \= dict\.get\(sum\)\n          if\(precalc\.isDefined\)\{\n            precalc\.get\n          \}else\{\n            val results \= \(limit to 1 by \-1\)\.map\(x \=\&gt; \{\n              val y \= pow\(x\, N\)\.toInt\n              if \(y \=\= sum\) 1 else numWays\(sum \- y\, x \- 1\)\n            \}\)\n            val powerSum \= results\.sum\n            \/\/try to remember\n            memoize\(PowerSumResult\(sum\, limit\, powerSum\)\)\n            powerSum\n          \}\n          \n          \}\n        \}\n      \}\n    \}\n    numWays\(X\, Integer\.MAX_VALUE\)\n  \}\n\n  private def memoize\(psr\: PowerSumResult\) \= \{\n    \/\/    dict\.get\(psr\.number\) match \{\n    \/\/      case Some\(x\) \=\&gt; println\(s\&quot;already in memory \$x\&quot;\)\n    \/\/      case None \=\&gt; \{\n    \/\/        val pq \= new mutable\.PriorityQueue\[PowerSumResult\]\(\)\n    \/\/        pq\.enqueue\(psr\)\n    \/\/        dict\.put\(psr\.number\, pq\)\n    \/\/      \}\n    \/\/    \}\n    dict\.getOrElseUpdate\(psr\.number\, psr\)\n  \}\n\}\n</find>
      <find>unm</find>
      <find>print</find>
    </findStrings>
  </component>
